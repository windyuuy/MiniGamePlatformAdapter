{
	"$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"module": "none",
		"target": "es5",
		"lib": [
			"es6",
			"dom",
			"es2015.promise"
		],
		"preserveConstEnums": true,
		"sourceMap": true,
		"outFile": "../../../dist/develop.js",
		"declaration": true,
		"experimentalDecorators": true,
		"composite": true,
		"skipLibCheck": true
	},
	"references": [
		{ "path": "../../fsync/src/lang" },
		{ "path": "../../fsync/src/fsync" }
	],
	"compileOnSave": true,
	"typeAcquisition": {}
}