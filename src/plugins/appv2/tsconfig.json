{
	"compilerOptions": {
		"module": "none",
		"target": "ES2017",
		"lib": [
			"es6",
			"dom",
			"es2015.promise"
		],
		"experimentalDecorators":true,
		"outFile": "../../../dist/appv2/appv2.js",
		"declaration": true,
		"composite": true,
		"skipLibCheck": true,
		"rootDirs": [
			"./",
			"../../framework",
			"../../lpluginshare/appshare"
		]
	},
	"compileOnSave": true,
	"typeAcquisition": {},
	"include": [
		"../../libs/**/*.ts",
		"../../framework/**/*.ts",
		"../../lpluginshare/appshare/**/*.ts",
		"./**/*.ts"
	],
	"typelua": {
		"extName": ".js"
	}
}