{
	"compilerOptions": {
		"module": "none",
		"target": "es5",
		"lib": [
			"es6",
			"dom",
			"es2015.promise"
		],
		"preserveConstEnums": true,
		"sourceMap": true,
		"outFile": "../../../dist/unityapp/unityapp.lua",
		"declaration": true,
		"composite": true,
		"rootDir": "./",
		"rootDirs": [
			"../../libs",
			"../../framework"
		]
	},
	"compileOnSave": true,
	"typeAcquisition": {},
